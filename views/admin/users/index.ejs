<%- include('../../partials/header.ejs') %>
<%- include('../../partials/navbar.ejs') %>

<body>
    <div class="container">
        <hr>      
            <h2> Usuários </h2>
            <a class="btn btn-success" href="/admin/users/create">Cadastrar usuários</a>  
            <hr>
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Email</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(item => { %>  
                <tr>
                    <th><%= item.id %></th>
                    <th><%= item.email %></th>
                </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>


<%- include('../../partials/footer.ejs') %>
